<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml" >
    <GridPane prefHeight="100.0" prefWidth="390.0" styleClass="vector-edit-grid" vgap="5.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <children>
            <Label fx:id="topLabel"  text="&lt;TOPLABELTEXT&gt;: " />
            <AnchorPane prefHeight="52.0" prefWidth="390.0" GridPane.rowIndex="1">
                <children>
                    <Label fx:id="fieldLabel" prefHeight="30.0" prefWidth="35.0" text="&lt;FIELDLABELTEXT&gt;: " />
                    <Label fx:id="valueLabel" prefHeight="30.0" prefWidth="200.0" text="&lt;VALUETEXT&gt;" />
                    <Button onAction="#handleNeedEditWindow" layoutX="290.0" layoutY="-1.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Edit..." />
                </children>
            </AnchorPane>
        </children>
    </GridPane>
</fx:root>
