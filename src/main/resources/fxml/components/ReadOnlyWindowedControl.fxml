<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <GridPane prefHeight="100.0" prefWidth="390.0" styleClass="vector-edit-grid" vgap="5.0">
    <children>
      <Label fx:id="topLabel" text="&lt;TOPLABELTEXT&gt;: " />
      <AnchorPane prefHeight="40.0" prefWidth="390.0" GridPane.rowIndex="1">
        <children>
          <Label fx:id="fieldLabel" layoutX="0.0" layoutY="0.0" prefHeight="47.0" prefWidth="103.0" text="&lt;FIELDLABELTEXT&gt;: " />
          <Label fx:id="valueLabel" layoutX="95.0" layoutY="0.0" prefHeight="47.0" prefWidth="195.0" text="&lt;VALUETEXT&gt;" />
          <Button layoutX="290.0" layoutY="-1.0" mnemonicParsing="false" onAction="#handleNeedEditWindow" prefHeight="47.0" prefWidth="100.0" text="Edit..." />
        </children>
      </AnchorPane>
    </children>
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
  </GridPane>
</fx:root>
